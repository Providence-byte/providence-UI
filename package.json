{
  "name": "providence-byte-ui",
  "version": "0.1.2",
  "description": "React Component Library",
  "author": "providence zhang",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "license": "MIT",
  "keywords": [
    "React",
    "TypeScript",
    "UI",
    "Conponent"
  ],
  "homepage": "https://github.com/Providence-byte/providence-UI",
  "repository": {
    "type": "git",
    "directory": "https://github.com/Providence-byte/providence-UI"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.2",
    "web-vitals": "^1.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "build": "yarn clean && yarn build-ts && yarn build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css --output-style compressed -o dist/css/ scss --recursive",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublish": "yarn test:nowatch && yarn lint && yarn build"
  },
  "husky":{
    "hooks":{
      "pre-commit":"yarn test:nowatch && yarn lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-controls": "^6.4.8",
    "@storybook/addon-docs": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.4.8",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "4.2.3",
    "@types/storybook__addon-info": "5.2.1",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.6",
    "husky": "^7.0.4",
    "node-sass": "^7.0.0",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.44.0",
    "sass-loader": "10.1.1",
    "storybook-addon-react-docgen": "^1.2.42",
    "style-loader": "2.0.0",
    "typescript": "^4.1.2",
    "webpack": "4.44.2"
  }
}
